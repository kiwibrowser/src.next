// ARIA reflection
// https://w3c.github.io/aria/#idl-interface
interface mixin AccessibilityRole {
  [CEReactions, Reflect] attribute DOMString? role;
};

Element includes AccessibilityRole;
ElementInternals includes AccessibilityRole;
